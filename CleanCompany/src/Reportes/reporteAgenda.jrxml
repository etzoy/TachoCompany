	<queryString>
		<![CDATA[SELECT c.nombre AS cliente, p.nombre as servicio, t."unidadCostoServicio",t."valorCostoServicio",CAST(t.fecha AS VARCHAR(12)),EXTRACT(HOUR FROM  "horaInicio")||' : '||EXTRACT(MINUTE FROM "horaInicio") as "horaInicio"  , EXTRACT(HOUR FROM  "horaFin")||' : '||EXTRACT(MINUTE FROM "horaFin") as "horaFin"
	FROM "cleanCompany"."ProgramacionServicio" t
	INNER JOIN "cleanCompany".cliente c ON t."idCliente"=c."idCliente"
	INNER JOIN "cleanCompany".servicio p ON t."idServicio"=p."idServicio"
	WHERE (fecha >= current_date and t.eliminado='0')
order by (fecha,  "horaInicio")
;]]>
	</queryString>
